img/%.png: 
	@mkdir -p $(@D)
	./plotter.R $^ $@

%.alignb: %.align
	brotli < $< > $@
	rm $<

%.align: %.alignb
	brotli --decompress < $< > $@

%_flen.align:
	@mkdir -p $(@D)
	./multialign/main maxsim1 $^ $@

%_truth.align:
	@mkdir -p $(@D)
	./multialign/main truth $^ $@

%_nflen.align:
	@mkdir -p $(@D)
	./multialign/main maxsim1 $^ $@

seq/%_flen.json: 
	@mkdir -p $(@D)
	./fktlen.py $< $@ 0

seq/%_truth.json: 
	@mkdir -p $(@D)
	./fktlen.py $< $@ 0

seq/%_nflen.json:
	@mkdir -p $(@D)
	./fktlen.py $< $@ 1

